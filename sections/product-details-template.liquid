  .select .selector-wrapper:first-child {
    display: none !important;
  }
  .swatch.clearfix:nth-child(2) {
    display: none;
  }
  .swatch .swatch-element {
    border: .125rem solid #e6e6e6;
    padding: .5625rem 1.875rem .75rem;
    font-size: 1rem;
    line-height: 1.333;
    text-align: center;
    color: #2a2a2a;
    background-color: #fff;
    cursor: pointer;
    margin-right: -2px;
    border-radius: .25rem;
    }

  .swatch_options input:checked+.swatch-element {
    z-index: 1;
    border-color: #6d80b4;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    box-shadow: none;
  }

  .swatch_options input:checked+.swatch-element:last-child {
    border-top-right-radius: .25rem;
    border-bottom-right-radius: .25rem;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
  }

  .swatch .swatch-element:focus {
    outline: none;
  }
  select {
    font-weight: bold;
    font-size: 15px;
    color: #2a2a2a;
  }


